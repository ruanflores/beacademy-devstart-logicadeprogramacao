algoritmo "Aula09Ex01"
// Função : Exive a tabuada de um número, dessa vez usando o Enquanto
// Autor : sproutjourney

var
c, numero: inteiro

inicio

      Escreva("Digite o número que deseja saber a tabuada: ")
      Leia(numero)
      Enquanto c < 11 faca
         EscrevaL(numero, " x", c, " =", (numero*c))
         c <- c + 1
      FimEnquanto
fimalgoritmo
